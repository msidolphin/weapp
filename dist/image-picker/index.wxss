.ellipsis{text-overflow:ellipsis;overflow:hidden;white-space:nowrap}.i-image-upload{width:100%;height:100%;padding-left:20rpx;width:inherit;display:flex;align-items:center;flex-flow:wrap}.i-image-upload-item{max-width:initial;max-height:initial;position:relative;display:flex;justify-content:center;align-items:center;flex-direction:column;text-align:center;box-sizing:border-box;border-style:solid;border-width:0}.i-image-upload-item .inner{position:absolute;top:0;right:20rpx;left:0;bottom:20rpx;border-radius:6rpx;display:flex;align-items:center;justify-content:center;background:#fff}.i-image-upload-item image{width:100%;height:100%;border-radius:6rpx}.i-image-upload-item__del{color:#fff;position:absolute;right:-10rpx;top:-10rpx;width:40rpx;height:40rpx;line-height:40rpx;text-align:center;border-radius:50%;background-color:rgba(0,0,0,.5)}.i-image-upload-item__del view{display:flex;width:40rpx;height:40rpx;align-items:center;justify-content:center}